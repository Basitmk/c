// Online C compiler to run C program online
#include <stdio.h>

void main() {
    int i;
    int n;
    printf("enter your number");
    scanf("%d",&n);
    for(i=1;i<=10;i++)
    {
        printf("%d*%d=%d\n",n,i,n*i);
    
        
    }
}
