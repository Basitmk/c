// Online C compiler to run C program online
//find the gretest number
#include <stdio.h>

void main() 
{
    int a,b,c;
   
    printf("enter your 1st number: ");
    scanf("%d",&a); 
    printf("\nenter your 2nd number: ");
    scanf("%d",&b);
    printf("\nenter your 3rd number: ");
    scanf("%d",&c);
    if(a>b&&a>c)
        printf("\na number is gretest: ");
     else if(b>c)
         printf("\nb number is gretest: ");
     else 
        printf("\nc number is gretest: ");
    
        
          
      
}
output
